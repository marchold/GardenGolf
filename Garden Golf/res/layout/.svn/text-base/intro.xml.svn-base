<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:background="@drawable/intro_background"
  android:id="@+id/introLayout">
  


<TextView android:layout_centerHorizontal="true" android:singleLine="true" android:textColor="#FF000000" android:paddingRight="80dip" android:paddingLeft="80dip"  android:paddingTop="5dip" android:textSize="38sp" android:text="@string/app_name" android:id="@+id/Title" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>

<TextView android:textColor="#FF000000" android:textSize="19sp" android:paddingRight="45dip" android:paddingLeft="45dip" android:paddingTop="60dip" android:text="@string/Intro" android:id="@+id/body" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>


<TextView android:textSize="30dip" android:textColor="#FFf13c1d" android:paddingLeft="50dip" android:layout_alignParentBottom="true" android:text="@string/whatsonroof" android:id="@+id/whatsonroof" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
</RelativeLayout>
